{
  "name": "template",
  "version": "1.2.1",
  "scripts": {
    "dev": "DEBUG=* rm -fr .next ; next dev",
    "build": "rm -fr .next ; next build",
    "export": "npm run build && next export",
    "format": "prettier --write \"src/**/*.tsx\" \"src/**/*.ts\"",
    "start": "npm run build && NODE_ENV=production next start -p 80",
    "lint": "next lint",
    "prepare": "husky install",
    "test": "echo \"Error: no test specified\"",
    "test:coverage": "echo \"Something like: vitest run --coverage\""
  },
  "engines": {
    "node": "v18.15.0"
  },
  "dependencies": {
    "next": "^13.4.3",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "20.2.4",
    "@types/react": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-primer-react": "^3.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "license": "MIT",
  "author": "Renato <renatobmpsilva@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renatobmps/template_web"
  },
  "bugs": {
    "url": "https://github.com/renatobmps/template_web/issues"
  },
  "homepage": "https://github.com/renatobmps/template_web#readme"
}
